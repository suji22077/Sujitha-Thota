#include<stdio.h>
int main()
{
    int a[100],n,i;
    float d=0,b=0,c=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]>0)
        {
          b=b+1;  
        }
        else if(a[i]<0)
        {
            c=c+1;
        }
        else 
         {
           d=d+1;
        }
    }
    b=b/n;
    c=c/n;
    d=d/n;
    printf("%.6f\n %.6f\n %.6f",b,c,d);
    return 0;
}
