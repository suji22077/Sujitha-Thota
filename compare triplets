#include <stdio.h>
int main()
{
    int a[3],b[3],i,count=0,count1=0;
    for(i=0;i<=2;i++)
    {
     scanf("%d",&a[i]);
    }
    for(i=0;i<=2;i++)
    {
     scanf("%d",&b[i]);
    }
    for(i=0;i<=2;i++)
    {
       if(a[i]>b[i])
        {
          count++;
        }
        else if(a[i]<b[i])
        {
          count1++;
        }
    }
    printf("%d %d",count,count1);
    return 0;
}
